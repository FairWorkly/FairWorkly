{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=TestDb;Username=YOUR_USER;Password=YOUR_PASSWORD"
  },
  "AiSettings": {
    "BaseUrl": "http://localhost:8000",
    "UseMockAi": true
  },
  "JwtSettings": {
    "Secret": "integration-test-secret-key-at-least-32-characters-long",
    "Issuer": "FairWorkly.Auth",
    "Audience": "FairWorkly.Web",
    "ExpiryMinutes": 15,
    "RefreshTokenExpiryDays": 7
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": []
  },
  "FileLogging": {
    "Enabled": false,
    "Path": "logs/test-.log"
  }
}
